    "eslint-plugin-import": "^2.29.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "prisma": "^6.18.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
