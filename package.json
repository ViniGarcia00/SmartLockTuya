    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^16.0.0",
    "node-mailer": "^0.1.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "string-similarity": "^4.0.4",
    "uuid": "^13.0.0",
    "winston": "^3.14.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
